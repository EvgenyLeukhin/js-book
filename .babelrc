{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": { "browsers": [ "last 2 versions", "> 0.5%", "not ie <= 10" ] },
        "debug": true
      }
    ],
    "@babel/preset-react"
  ],

  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties"
  ],

  "ignore": [ "/node_modules/" ]
}
